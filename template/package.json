{
  "name": "<%= projectName %>",
  "version": "1.0.0",
  "description": "<%= projectDescription %>",
  "scripts": {
    "dev": "webpack-dev-server --config build-utils/webpack.config.js --env.env=dev",
    "build": "webpack --config build-utils/webpack.config.js --env.env=prod",
    "build:analyze": "yarn build -- --env.addon=bundleanalyze --env.addon=bundlevisualizer",
    "test": "jest",
    "test:coverage": "jest --coverage"
  },
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/<%= username %>/<%= projectName %>.git"
  },
  "author": "<%= displayname %> <<%= email %>> (http://github.com/<%= username %>)",
